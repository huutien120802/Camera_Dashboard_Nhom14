{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/prefer-default-export": "off",
    "import/extensions": "off",
    "consistent-return": "off",
    "no-underscore-dangle": "off",
    "linebreak-style": 0
  }
}
